		BLOCK "040904b0"
		BEGIN
			VALUE "Comments", "For more information about " APPLICATION " visit: " APP_URL "  Made using the Qfusion engine: https://qfusion.net/\0"
			VALUE "CompanyName", APP_COPYRIGHT_OWNER "\0"
			VALUE "FileDescription", APPLICATION "\0"
			VALUE "FileVersion", APP_VERSION "\0"
			VALUE "LegalCopyright", "Copyright © 2018\0"
			VALUE "LegalTrademarks", APP_COPYRIGHT_OWNER "\0"
			VALUE "ProductName", APPLICATION "\0"
			VALUE "ProductVersion", APP_VERSION "\0"
		END
	END
	BLOCK "VarFileInfo"
	BEGIN
		VALUE "Translation", 0x409, 1200
	END
END
